
public class DailyWagesEmp extends Employees{
	
	private double salary,gross;
	public DailyWagesEmp(int empid,String empname,double salary){
		super();
		this.empid=empid;
		this.empname=empname;
		this.salary=salary;
		
	}
		public void calculateDailySalary(){
			gross=salary*30;
			
		
	}
		@Override
		public String toString() {
			return "gross=" + gross ;
		}
		
		
}
