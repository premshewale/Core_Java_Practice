
public abstract class Rectangle extends MyShape {
	 private int breadth,length;
	 private float area;
	
	public Rectangle(int breadth, int length) {
		
		this.breadth = breadth;
		this.length = length;
	}

	public void area(){
		area=length*breadth;
	}

	@Override
	public String toString() {
		return "Rectangle [area=" + area + "]";
	}


}
