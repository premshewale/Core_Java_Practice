
public class Evenodd {

	private int number,even,odd;
	
	public Evenodd(int number){
		this.number=number;
	}
	public void checkEvenOdd(){
		if(number%2==0){
			even=this.number;
		}
		else{
			odd=this.number;
		}
	}
	
	public String toString(int even){
		return "Number is Even :"+even;
		
	}	
	public String toString(int odd){
		return "Number is Even :"+odd;
}
